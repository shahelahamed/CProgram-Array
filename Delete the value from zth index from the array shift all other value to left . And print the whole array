#include<stdio.h>
int main()
{
    int arr[100]={5,8,4,1,2,4,7,6,9,10};
    int i,y;
    printf("Enter a index for delete :  ");
    scanf("%d",&y);


    for(i = 0; i< 99; i++)
    {
       if(i>=y)
       {
           arr[i]=arr[i+1];
       }
    }
    for(i = 0; i< 100; i++)
    {
        printf("%d\n",arr[i]);
    }
    return 0;
}



