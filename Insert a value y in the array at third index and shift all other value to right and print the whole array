#include<stdio.h>
int main()
{
    int arr[100]={5,8,4,1,2,4,7,6,9,10};
    int i,y;
    printf("Enter a value Y:  ");
    scanf("%d",&y);

    for(i = 99; i > 3; i--)
    {
        arr[i]=arr[i-1];
    }
     arr[3] = y;
    for(i = 0; i< 100; i++)
    {
        printf("%d\n",arr[i]);
    }
    return 0;
}


